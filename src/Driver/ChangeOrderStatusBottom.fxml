<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>

<!--> Bottom bar with buttons in submenu "Change order status" </!-->


<HBox fx:id="bottomHBox" prefHeight="80.0" prefWidth="100.0" alignment="CENTER" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Button fx:id="deliveredButton" HBox.hgrow="ALWAYS" mnemonicParsing="false" text="Mark as delivered" />
        <Button fx:id="notDeliveredButton" HBox.hgrow="ALWAYS" mnemonicParsing="false" text="Mark as not delivered" />
    </children>
</HBox>
