                </graphic>
            </Button>
            <Button fx:id="subsButton" mnemonicParsing="false" text="Subscriptions" textAlignment="CENTER" styleClass="TopButton">
                <graphic>
                    <ImageView>
                        <Image url="@/labels/Renew Subscription.png" />
                    </ImageView>
                </graphic>
            </Button>
            <Button fx:id="customersButton" mnemonicParsing="false" text="Customers" textAlignment="CENTER" styleClass="TopButton">
                <graphic>
                    <ImageView>
                        <Image url="@/labels/salesLabels/User Group Woman Woman.png" />
                    </ImageView>
                </graphic>
